
<%= render 'places-header' %>

<div class="places-headroom"></div>

<div class="places-container">

  <%= link_to 'Create New Place', new_place_path, :class => "create_places_button" %>

  <%= gmaps4rails(@json) %>

  <% @places.each do |places| %>

  <div class="places-preview-list-box">
        <div class="placelist" >
          <h3><%= place.name %></h3>
            <div class="places-edit-checkbox">
              <%= link_to '', edit_places_path(places), :class => "places-edit-sprite" %>
            </div><!--end  checkbox-->
        <div class="places-delete-checkbox">
            <%= link_to '', places, :class => "places-delete-sprite", method: :delete, data: { confirm: 'Are you sure you want to delete this place?' } %>
        </div><!--end edit checkbox-->
    </div> <!-- end placelist -->

 </div> <!-- end places previewlistbox -->

</div>

<% end %>

<%= render 'places-footer' %>