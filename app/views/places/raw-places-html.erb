<%= form_for :places do |f| %>

  <%= f.text_field :place_name, :placeholder => " ex. 'Dentist', 'Work', 'Grandma's House'"  %><br />

  <%= f.text_field :place_address, :placeholder => " ex. Hemophelia" %><br />

  <%= f.text_field :place_url, :placeholder => " ex. Hemophelia" %><br />

  <%= f.label :edit, "Edit" %><br />
  <%= f.submit :edit %><br />

  <%= f.label "Save" %><br />
  <%= f.submit %><br />

  <% end %>