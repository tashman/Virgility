           <div class="formbox-wrapper">

                     <h3>Place Name</h3>

                    <form>
                      <input type="text" name="place_name" class="formbox">
                           <% form_for @user do |f| %>
                           <%= f.text_field :place_address, :placeholder => " ex. '34 Valley Road, Walla Walla, WA 99362'" %>
                            <% end %>
                    </form>

                    <br />
            </div> <!--end formbox-wrapper-->
            
             <div class="clear"></div>
                    
            <div class="formbox-wrapper">

                   <h3>Address</h3>

                    <form>
                      <input type="text" name="place_address" class="formbox">
                       <% form_for @user do |f| %>
                        <%= f.text_field :place_name, :placeholder => " ex. 'Dentist', 'Work', 'Grandma's House'" %>
                        <% end %>
                    </form>

                    <br />

             </div> <!--end formbox-wrapper-->
             
              <div class="clear"></div>
                    

         <div class="buttonbox">
         
              <form>
                <input type="submit" class="button" value="Preview">
                   <% form_for @user do |f| %>
